<!DOCTYPE html>
<html ng-app="myModule">
<head lang="en">
    <meta charset="UTF-8">
    <title>Turret Dashboard</title>
    <!-- load bootstrap css First -->

    <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">
    

    <!-- Then load menu bar css -->
    <link rel="stylesheet" href="/css/custom.css">



    <!--Including bootswatch theme CSS -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">

    <!-- results background css -->
    <link rel="stylesheet" href="/css/resultBackGround.css">


    <!-- animation css -->
    <link rel="stylesheet" href="/css/animate.css">

    
    <!-- Java scripts -->
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script> 
    <script src="/bower_components/angular/angular.min.js"></script>
    <script src="/bower_components/angular-route/angular-route.min.js"></script>    
    <script src="/bower_components/Chart.js/Chart.min.js"></script>
    <script src="/bower_components/tc-angular-chartjs/dist/tc-angular-chartjs.min.js"></script>
    <script src="/bower_components/angularUtils-pagination/dirPagination.js"></script>
    <script src="/bower_components/angular-sanitize/angular-sanitize.js"></script>
    <!-- script src="/bower_components/angular-growl-notifications/dist/angular-growl-notifications.js"></script -->
    <script src="/bower_components/angular-animate/angular-animate.min.js"></script>

    <!-- Amcharts JS -->
    <script type="text/javascript" src="/bower_components/amcharts/dist/amcharts/amcharts.js"></script>
    <script type="text/javascript" src="/bower_components/amcharts/dist/amcharts/serial.js"></script>

    <script type="text/javascript" src="/bower_components/amcharts-angular/dist/amChartsDirective.js"></script>

    <!-- Angular Controllers -->
    <script src="/controllers/controller.js"></script>

    <!-- Socket.io JS -->
    <script src="/bower_components/socket.io-client/socket.io.js"></script>
    
</head>


<body>

<!--Heading and Navbar-->
<div id="wrapper">

    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div  class="navbar-brand">
                    <a id="menu-toggle" href="#" class="glyphicon glyphicon-align-justify btn-menu toggle">
                        <i class="fa fa-bars"></i>
                    </a>
                    <a href="#home">Turret Dashboard</a>
                </div>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#home">Home</a></li>
                    <li><a href="#about">About</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>

   <!-- Sidebar -->
    <div id="sidebar-wrapper">
        <nav id="spy">
            <ul class="sidebar-nav nav">
                
                
                <li>
                    <a href="#charts">
                        <span>Charts</span>
                    </a>
                </li>
               <li>
                    <a href="#results">
                        <span>Results</span>
                    </a>
                </li>
                <li>
                    <a href="#bugList">
                        <span>Bugs</span>
                    </a>
                </li>

                <li>
                  <a href="#dash">
                        <span>Dash</span>
                    </a>  
                </li>
                <p>
                    <a class="btn btn-info btn-block" href="/users/logout">
                        <span class="glyphicon glyphicon-log-out"></span> Sign Out
                    </a>
                </p>
                
            </ul>
        </nav>
    </div>
    <!-- Page content -->
    <div id="page-content-wrapper">
        <div class="page-content">



<!-- Angular views will load here -->
        <div id="main">

         <!-- Specifying seperate class for each page --> 
        <div class="page {{ pageClass }}" ng-view></div>


<!-- Footer for Page content -->
        </div>
    </div>
</div>
    <script src="/controllers/menu.js"></script>        
</body>
</html>